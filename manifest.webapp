{
  "version": "1.0.0",
  "name": "Bouncy",
  "launch_path": "/index.html",
  "description": "Click to make the ball jump. Pass through the walls when they are the same color.",
  "subtitle": "Make the ball match the wall",
  "type": "web",
  "icons": {
    "56": "/icons/44-x-44-px.png",
    "112": "/icons/112-x-112-px.png"
  },
  "developer": {
    "name": "Eyepax",
    "url": "https://www.eyepax.com"
  },
  "dependencies": {
		"ads-sdk": "1.3.0"
	},
  "installs_allowed_from": [
    "*"
  ],
  "default_locale": "en-US",
  "categories": [
    "sports"
  ],
  "permissions": {
    "desktop-notification": {
      "description": "To show notifications"
    },
    "geolocation": {
      "description": "Marking out user location"
    },
    "alarms": {
      "description": "Scheduling alarms"
    }
  },
  "fullscreen": "true",
  "locales": {
     "en-US": {
      "name": "Bouncy",
      "subtitle": "Make the ball match the wall",
      "description": "Click to make the ball jump. Pass through the walls when they are the same color."
    }
  }
}